[begin] (RTS Live)
   [submenu] (Applications) 
      [exec] (Console)  {x-terminal-emulator  -T "Bash" -e sh -c "/bin/bash --login"} 
      [exec] (Ansible)  {x-terminal-emulator  -T "Bash" -e sh -c "cd /root/RTS_ansible; /bin/bash"} 
      [exec] (Web Browser)  {surf http://localhost} 
   [end]
[nop] ()
[exit] (Exit) 

[end]
